<h1>Listing worklogs</h1>

<table>
  <tr>
    <th>Who</th>
    <th>Work progress this week</th>
    <th>Concerns</th>
    <th>Filed on</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @worklogs.each do |worklog| %>
  <tr>
    <td><%= worklog.who %></td>
    <td><%= worklog.work_progress_this_week %></td>
    <td><%= worklog.concerns %></td>
    <td><%= worklog.filed_on %></td>
    <td><%= link_to 'Show', worklog %></td>
    <td><%= link_to 'Edit', edit_worklog_path(worklog) %></td>
    <td><%= link_to 'Destroy', worklog, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Worklog', new_worklog_path %>
